{
  "name": "application-website",
  "private": true,
  "type": "module",
  "scripts": {
    "build": "nuxt build",
    "check:build": "sh ./scripts/check-build.sh",
    "check:commits": "sh ./scripts/check-commits.sh",
    "check:types": "vue-tsc --noEmit",
    "cleanup:icons": "sh ./scripts/cleanup-icons.sh",
    "dev": "nuxt dev --dotenv .env.local",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "generate": "nuxt generate",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "postinstall": "nuxt prepare",
    "prepare": "husky",
    "preview": "nuxt preview --dotenv .env.production"
  },
  "devDependencies": {
