    "prepare": "husky",
    "icon:cleanup-svgs": "sh ./scripts/icon-cleanup-svgs.sh",
    "icon:generate-sprite": "sh ./scripts/icon-generate-sprite.sh",
    "component:merge": "sh ./scripts/component-merge.sh",
    "build:trigger": "sh ./scripts/build-trigger.sh"
