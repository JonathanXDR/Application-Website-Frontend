    "@nuxt/devtools": "^1.7.0",
    "@nuxt/eslint": "^1.0.0",
    "@nuxt/fonts": "^0.10.3",
    "@nuxt/icon": "^1.10.3",
    "@nuxt/image": "^1.9.0",
    "@nuxt/scripts": "^0.9.5",
    "@nuxtjs/color-mode": "^3.5.2",
    "@nuxtjs/i18n": "^9.1.5",
    "@nuxtjs/seo": "^2.1.1",
